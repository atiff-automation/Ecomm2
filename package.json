{
  "name": "jrm-ecommerce",
  "version": "0.1.0",
  "private": true,
  "description": "JRM E-commerce Platform with Malaysian Membership System",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "npm run db:deploy:production && next start",
    "start:production": "next start",
    "db:deploy:production": "npx prisma migrate deploy && npm run db:seed:production",
    "db:seed:production": "npm run db:seed:essential && npm run db:seed:postcodes:production && npm run db:seed:shipping:production",
    "db:seed:essential": "NODE_ENV=production npx tsx prisma/seed.ts",
    "db:seed:postcodes:production": "NODE_ENV=production npx tsx prisma/seed-malaysian-postcodes.ts",
    "db:seed:shipping:production": "NODE_ENV=production npx tsx prisma/seed-shipping-zones.ts",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "db:seed": "npx tsx prisma/seed.ts && node scripts/post-seed-cleanup.js",
    "db:seed:postcodes": "npx tsx prisma/seed-malaysian-postcodes.ts",
    "db:reset": "npx prisma migrate reset --force && npm run db:seed && npm run db:seed:postcodes",
    "session:clear": "curl -X POST http://localhost:3000/api/auth/clear-session",
    "db:backup": "./scripts/db-backup.sh",
    "db:restore": "./scripts/db-restore.sh",
    "db:migrate": "./scripts/safe-migrate.sh",
    "db:studio": "npx prisma studio",
    "chat:queue": "npx tsx scripts/queue-worker.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "analyze": "ANALYZE=true npm run build",
    "analyze:dev": "ANALYZE=true npm run dev",
    "bundle:size": "npm run build && npx size-limit",
    "bundle:why": "npx whybundled .next/static/chunks/pages/**/*.js",
    "perf:audit": "npm run build && npx lighthouse http://localhost:3000 --only-categories=performance --chrome-flags=\"--headless\"",
    "optimize:images": "npx next-optimized-images",
    "check:deps": "npx depcheck",
    "check:unused": "npx unimported",
    "stats": "npm run build -- --profile --json > bundle-stats.json",
    "postcode:import": "npx tsx scripts/import-postcodes.ts",
    "postcode:migrate": "npx prisma migrate dev --name add_malaysian_postcode_system",
    "postcode:validate": "npx tsx scripts/import-postcodes.ts --validate-only"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@react-email/components": "^0.4.0",
    "@sentry/nextjs": "^10.1.0",
    "@types/archiver": "^6.0.3",
    "@types/jszip": "^3.4.0",
    "@types/node-cron": "^3.0.11",
    "@types/papaparse": "^5.3.16",
    "@types/pdfkit": "^0.17.3",
    "@types/puppeteer": "^5.4.7",
    "@types/speakeasy": "^2.0.10",
    "@types/xlsx": "^0.0.35",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.4",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.536.0",
    "next": "14.2.31",
    "next-auth": "^4.24.11",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.17.2",
    "prisma": "^6.16.2",
    "puppeteer": "^24.16.1",
    "react": "^18",
    "react-colorful": "^5.6.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18",
    "react-email": "^4.2.8",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.2",
    "resend": "^4.8.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "overrides": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.31",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
